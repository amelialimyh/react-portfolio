ter/components/App.js","original":{"line":40,"column":32}},{"name":"tracks","generated":{"line":112,"column":27},"source":"projects/music-master/components/App.js","original":{"line":40,"column":43}},{"generated":{"line":113,"column":0},"source":"projects/music-master/components/App.js","original":{"line":40,"column":16}},{"generated":{"line":113,"column":8},"source":"projects/music-master/components/App.js","original":{"line":36,"column":12}},{"generated":{"line":113,"column":9},"source":"projects/music-master/components/App.js","original":{"line":35,"column":8}},{"generated":{"line":114,"column":0},"source":"projects/music-master/components/App.js","original":{"line":43,"column":5}},{"name":"Component","generated":{"line":118,"column":2},"source":"projects/music-master/components/App.js","original":{"line":9,"column":18}},{"name":"App","generated":{"line":120,"column":15},"source":"projects/music-master/components/App.js","original":{"line":46,"column":15}}],"sources":{"projects/music-master/components/App.js":"import React, { Component } from 'react';\nimport Search from './Search';\nimport Artist from './Artist';\nimport Tracks from './Tracks';\n\n// it is a global constant hence it has to be screen cased\nconst API_ADDRESS = 'https://spotify-api-wrapper.appspot.com';\n\nclass App extends Component {\n    state = { artist: null, tracks: [] };\n\n    componentDidMount() {\n        this.searchArtist('pentatonix');\n    }\n\n    searchArtist = artistQuery => {\n        fetch(`${API_ADDRESS}/artist/${this.state.artistQuery}`)\n            .then(response => response.json())\n            .then(json => {\n\n                if (json.artists.total > 0) {\n                    const artist = json.artists.items[0];\n                    this.setState({ artist });\n\n                    fetch(`${API_ADRRESS}/artist/${artist.id}/top-tracks`)\n                        .then(response => response.json())\n                        .then(json => this.setState({ tracks: json.tracks }))\n                        .catch(error => alert(error.message));\n                }\n            })\n            .catch(error => alert(error.message));\n    }\n\n    render() {\n        return (\n            <div>\n                <h2>Music Master</h2>\n                <Search searchArtist={this.searchArtist} />             \n                <Artist artist={this.state.artist} />\n                <Tracks tracks={this.state.tracks}/>\n            </div>\n        );\n    }\n}\n\nexport default App;"},"lineCount":null}},"error":null,"hash":"cb1ac617b1cb9892eb1252e8a39318a2","cacheData":{"env":{}}}