inal":{"line":65,"column":32}},{"generated":{"line":131,"column":18},"source":"projects/music-master/components/Tracks.js","original":{"line":55,"column":28}},{"generated":{"line":131,"column":33},"source":"projects/music-master/components/Tracks.js","original":{"line":66,"column":32}},{"generated":{"line":132,"column":0},"source":"projects/music-master/components/Tracks.js","original":{"line":66,"column":35}},{"generated":{"line":132,"column":12},"source":"projects/music-master/components/Tracks.js","original":{"line":66,"column":35}},{"generated":{"line":132,"column":21},"source":"projects/music-master/components/Tracks.js","original":{"line":66,"column":44}},{"generated":{"line":132,"column":23},"source":"projects/music-master/components/Tracks.js","original":{"line":66,"column":45}},{"generated":{"line":133,"column":0},"source":"projects/music-master/components/Tracks.js","original":{"line":66,"column":32}},{"generated":{"line":133,"column":13},"source":"projects/music-master/components/Tracks.js","original":{"line":66,"column":59}},{"generated":{"line":133,"column":19},"source":"projects/music-master/components/Tracks.js","original":{"line":66,"column":63}},{"name":"trackIcon","generated":{"line":133,"column":20},"source":"projects/music-master/components/Tracks.js","original":{"line":66,"column":64}},{"generated":{"line":133,"column":29},"source":"projects/music-master/components/Tracks.js","original":{"line":66,"column":59}},{"name":"track","generated":{"line":133,"column":30},"source":"projects/music-master/components/Tracks.js","original":{"line":66,"column":74}},{"generated":{"line":133,"column":35},"source":"projects/music-master/components/Tracks.js","original":{"line":66,"column":59}},{"generated":{"line":133,"column":36},"source":"projects/music-master/components/Tracks.js","original":{"line":66,"column":32}},{"generated":{"line":133,"column":37},"source":"projects/music-master/components/Tracks.js","original":{"line":55,"column":28}},{"generated":{"line":134,"column":0},"source":"projects/music-master/components/Tracks.js","original":{"line":53,"column":24}},{"generated":{"line":135,"column":0},"source":"projects/music-master/components/Tracks.js","original":{"line":69,"column":21}},{"generated":{"line":135,"column":7},"source":"projects/music-master/components/Tracks.js","original":{"line":50,"column":20}},{"generated":{"line":135,"column":8},"source":"projects/music-master/components/Tracks.js","original":{"line":48,"column":12}},{"generated":{"line":135,"column":9},"source":"projects/music-master/components/Tracks.js","original":{"line":47,"column":8}},{"generated":{"line":136,"column":0},"source":"projects/music-master/components/Tracks.js","original":{"line":73,"column":5}},{"name":"Component","generated":{"line":140,"column":2},"source":"projects/music-master/components/Tracks.js","original":{"line":3,"column":21}},{"name":"Tracks","generated":{"line":142,"column":15},"source":"projects/music-master/components/Tracks.js","original":{"line":76,"column":15}}],"sources":{"projects/music-master/components/Tracks.js":"import React, { Component } from 'react';\n\nclass Tracks extends Component {\n    // add the boolean playing so that we can play and pause the audio depending on this playing boolean\n    // if it is false the audio will start to play but if it is true the audio will stop playing\n    state = { playing: false, audio: null, playingPreviewUrl: null };\n\n    // make audio return a function itself (double =>) so that it is going to point to its callback function <-- this prevents the loops of setting state and rendering from happening\n    playAudio = previewUrl => () => {\n        const audio = new Audio(previewUrl);\n\n        // check if it is currently false\n        if (!this.state.playing) {\n            audio.play();\n            this.setState({ playing: true, audio, playingPreviewUrl: previewUrl });\n        } else {\n            // refers to the current audio object and pause it\n            this.state.audio.pause();\n\n            if (this.state.playingPreviewUrl === previewUrl) {\n                this.setState({ playing: false });\n            } else {\n                audio.play();\n                this.setState({ audio, playingPreviewUrl: previewUrl });\n            }\n        }\n    }\n\n    trackIcon = track => {\n        if (track.preview_url) {\n            return <span>N/A</span>;\n        }\n\n        if (\n            this.state.playing &&\n            this.state.playingPreviewUrl === track.preview_url\n        ) {\n            return <span>| |</span>\n        }\n\n        return <span>&#9654;</span>\n    }\n\n    render() {\n        const { tracks } = this.props;\n\n        return (\n            <div>\n                {\n                    tracks.map(track => {\n                        const { id, name, album, preview_url } = track;\n\n                        return (\n                            // bad practice to use parentheses to pass in the preview_url cos it can lead to loops of setting state and rendering in our component\n                            <div\n                                key={id}\n                                onClick={this.playAudio(preview_url)}\n                                className='track'\n                            >\n                                <img\n                                    src={album.images[0].url}\n                                    alt='track-image'\n                                    className='track-image'\n                                />\n                                <p className='track-text'>{name}</p>\n                                <p className='track-icon'>{this.trackIcon(track)}</p>\n                            </div>\n                        )\n                    })\n                }\n            </div>\n        )\n    }\n}\n\nexport default Tracks;\n"},"lineCount":null}},"error":null,"hash":"833c1db54c1e4bc20ca6b379a2fefdfa","cacheData":{"env":{}}}